{
  "name": "tumblr-tools",
  "productName": "tumblr-tools",
  "version": "1.0.0",
  "description": "Tumblr Tools",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "db:migrate:status": "tsx migrate.ts pending && echo '-----' && tsx migrate.ts executed",
    "db:migrate": "tsx migrate.ts up",
    "db:rollback": "tsx migrate.ts down",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint",
    "migration:generate": "tsx migrate.ts create",
    "type-check": "tsc --noEmit",
    "start": "tsx index.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "zsolt",
  "license": "MIT",
  "config": {},
  "dependencies": {
