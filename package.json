    "async": "^3.2.6",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cld": "^2.10.1",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "grant": "^5.4.24",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "node-stream-zip": "^1.15.0",
    "pg": "^8.15.6",
    "pino": "^9.6.0",
    "reflect-metadata": "^0.2.2",
